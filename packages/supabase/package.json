{
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "@t3-oss/env-core": "^0.11.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/config-eslint": "workspace:*",
    "@repo/config-typescript": "workspace:*",
    "@types/node": "^20.17.6",
    "typescript": "latest"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "name": "@repo/supabase",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "generate-types": "pnpm run start-service && npx supabase gen types typescript --local > ./src/database.types.ts && pnpm run stop-service",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "start-service": "npx supabase start --ignore-health-check",
    "stop-service": "npx supabase stop"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
